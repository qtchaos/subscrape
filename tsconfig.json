{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "jsx": "react-jsx",
    "target": "es2021",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "jsxImportSource": "hono/jsx",
    "baseUrl": "./src",
    "resolveJsonModule": true,
    "paths": {
      "$lib/*": ["lib/*"]
    }
  }
}
